pipeline{
  agent  any

  stages {
    stage ('preparing the  Artifact') {
      steps{
       sh '''
         zip -r ../frontend.zip *
       '''
      }
    }
    stage('upload artifact'){
      steps{
        sh '''
          curl -f -v -u admin:admin123 --upload-file frontend.zip http://3.237.188.10:8081/repository/frontend/frontend.zip
       
        '''


      }

    }

  }
}
